{
  "name": "@0byte/stagehand-examples",
  "version": "1.0.8",
  "private": true,
  "description": "Example scripts for Stagehand",
  "main": "./",
  "scripts": {
    "build": "pnpm --filter @0byte/stagehand run build",
    "start": "pnpm run build && sh -c 'if [ -n \"$1\" ]; then tsx \"$1.ts\"; else tsx example.ts; fi' --"
  },
  "dependencies": {
    "@0byte/stagehand": "workspace:*"
  },
  "devDependencies": {
    "tsx": "^4.10.5",
    "jszip": "^3.10.1"
  }
}